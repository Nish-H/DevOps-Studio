# Vercel KV Redis Configuration
# Get these values from your Vercel dashboard -> Storage -> KV Redis
KV_REST_API_URL=your_vercel_kv_rest_api_url
KV_REST_API_TOKEN=your_vercel_kv_rest_api_token
KV_REST_API_READ_ONLY_TOKEN=your_vercel_kv_read_only_token

# Google Drive API Configuration
NEXT_PUBLIC_GOOGLE_CLIENT_ID=your_google_client_id_here
GOOGLE_CLIENT_SECRET=your_google_client_secret_here

# Optional: Custom app name for Google Drive folder
NEXT_PUBLIC_APP_NAME=Nishen-AI-Workspace

# Development vs Production
NODE_ENV=development